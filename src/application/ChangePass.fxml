<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="341.0" prefWidth="400.0" style="-fx-background-color: beige;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChangePassController">
   <children>
      <Label layoutX="53.0" layoutY="79.0" text="New Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="pswNew" layoutX="167.0" layoutY="75.0" promptText="New Password">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <Label layoutX="6.0" layoutY="126.0" text="Confirm New Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="pswConfirm" layoutX="167.0" layoutY="122.0" promptText="Confirm Password">
         <font>
            <Font size="13.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnOK" layoutX="52.0" layoutY="220.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="38.0" prefWidth="104.0" text="OK">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="238.0" layoutY="220.0" mnemonicParsing="false" onAction="#cancelButtonAction" prefHeight="38.0" prefWidth="94.0" text="Cancel">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
